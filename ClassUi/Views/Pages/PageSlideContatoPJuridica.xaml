<Page x:Class="ClassUi.Views.Pages.PageSlideContatoPJuridica"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ClassUi.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageSlideContatoPJuridica">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="15"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Image x:Name="Image1" MinWidth="500" MinHeight="300" Margin="5"/>
        <ProgressBar x:Name="proStatus" Grid.Row="1" Grid.Column="1" Background="Transparent" Foreground="#FF03ACAC" IsIndeterminate="False"></ProgressBar>
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="btnRetroceder" Content="Retroceder" Width="70" Height="35" Margin="5" Click="BtnRetroceder_Click"></Button>
            <Button x:Name="btnAvancar" Content="Avançar" Width="70" Height="35" Margin="5" Click="BtnAvancar_Click"></Button>
        </StackPanel>
    </Grid>
</Page>
